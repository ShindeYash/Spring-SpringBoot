
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="Dev" class="org.example.Dev">

    </bean>

    <bean id="Dev1" class="org.example.Dev">
        <property name="age" value="12"/>
    </bean>

    <bean id="Dev2" class="org.example.Dev">
        <constructor-arg value="99"/>
    </bean>

    <bean id="Dev3" class="org.example.Dev">
        <constructor-arg index = "0" value="1000"/>
        <constructor-arg index = "1" value="25000"/>
    </bean>

    <bean id="Lap" class="org.example.Laptop">

    </bean>

</beans>